<html>
  <head>
    <title>User Center</title>
    <noscript><meta http-equiv="refresh" content="5; url=&#39;<%= @back_url %>&#39;"></noscript>
    <script type="text/javascript">
      function doRedirect() {
        location.replace("<%= @back_url %>");
      }
    </script>
  </head>
  <body onload="doRedirect()">
  <p>Logging In, Please wait...</p>
  <% @sts.each do |st| %>
    <%= image_tag "http://#{st.service}:4000/privacy?ticket=#{st.ticket}", :size => "0x0" %>
  <% end %>
  </body>
</html>